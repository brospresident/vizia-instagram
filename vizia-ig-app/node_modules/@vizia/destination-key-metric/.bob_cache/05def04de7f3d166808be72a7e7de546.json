pe: 'ACTION_NAME', data: *}\n */\nfunction reducer(state, action) {\n    if (actionHandlers[action.type]) {\n        return actionHandlers[action.type](state, action.data);\n    }\n\n    return state;\n}\n\nmodule.exports = reducer;\n"],"mappings":"AAAA,YAAY;;AAEZ;AACA;AACA;AACA;AACA,SAASA,aAAa,CAACC,KAAK,EAAEC,KAAK,EAAE;EACjC,MAAMC,UAAU,GAAGC,MAAM,CAACC,IAAI,CAACJ,KAAK,CAACK,MAAM,CAAC;EAC5C,MAAMC,aAAa,GAAGH,MAAM,CAACC,IAAI,CAACH,KAAK,CAAC,CACnCM,MAAM,CAAEC,QAAQ,IAAKN,UAAU,CAACO,QAAQ,CAACD,QAAQ,CAAC,CAAC,CACnDE,MAAM,CAAC,CAACC,IAAI,EAAEH,QAAQ,KAAKL,MAAM,CAACS,MAAM,CAACD,IAAI,EAAE;IAAC,CAACH,QAAQ,GAAGP,KAAK,CAACO,QAAQ;EAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;EAEvF,IAAI,CAACL,MAAM,CAACC,IAAI,CAACE,aAAa,CAAC,CAACO,MAAM,EAAE;IACpC,OAAOb,KAAK;EAChB;EAEA,OAAOG,MAAM,CAACS,MAAM,CAAC,CAAC,CAAC,EAAEZ,KAAK,EAAE;IAC5BK,MAAM,EAAEF,MAAM,CAACS,MAAM,CAAC,CAAC,CAAC,EAAEZ,KAAK,CAACK,MAAM,EAAEC,aAAa;EACzD,CAAC,CAAC;AACN;;AAEA;AACA;AACA;AACA,SAASQ,WAAW,CAACd,KAAK,EAAEe,IAAI,EAAE;EAC9B,OAAOZ,MAAM,CAACS,MAAM,CAAC,CAAC,CAAC,EAAEZ,KAAK,EAAE;IAACe;EAAI,CAAC,CAAC;AAC3C;;AAEA;AACA,MAAMC,cAAc,GAAG;EACnBjB,aAAa;EACbe;AACJ,CAAC;;AAED;AACA;AACA;AACA;AACA,SAASG,OAAO,CAACjB,KAAK,EAAEkB,MAAM,EAAE;EAC5B,IAAIF,cAAc,CAACE,MAAM,CAACC,IAAI,CAAC,EAAE;IAC7B,OAAOH,cAAc,CAACE,MAAM,CAACC,IAAI,CAAC,CAACnB,KAAK,EAAEkB,MAAM,CAACH,IAAI,CAAC;EAC1D;EAEA,OAAOf,KAAK;AAChB;AAEAoB,MAAM,CAACC,OAAO,GAAGJ,OAAO"}]}